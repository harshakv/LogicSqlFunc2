    <UserSecretsId>2be3a385-741a-4a22-8863-eb02cb3f7cd4</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.7" />
